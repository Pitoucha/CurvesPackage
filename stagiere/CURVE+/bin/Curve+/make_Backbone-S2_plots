cat > Backbone-S2_param_Ampli.plt <<EOF
set terminal postscript eps enhanced color solid "Times-Roman" 16
set output 'Backbone-S2_Ampli.eps'

set pm3d map  
set title 'Ampli'
set palette model RGB
set xlabel 'time'
set ylabel 'pair number'

splot 'Backbone-S2_Parameters' u 1:2:11 with pm3d notitle

EOF

gnuplot Backbone-S2_param_*.plt

for f in *eps ; do ps2pdf $f ; done

