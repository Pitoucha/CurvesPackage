#!/bin/bash

cat > Backbone-S2_param_Alpha.plt <<EOF
set terminal postscript eps enhanced color solid "Times-Roman" 16
set output 'Backbone-S2_Alpha.eps'

set pm3d map  
set title 'Alpha'
set palette model RGB
set xlabel 'time'
set ylabel 'pair number'

splot 'Backbone-S2_Parameters' u 1:2:3 with pm3d notitle

EOF

cat > Backbone-S2_param_Beta.plt <<EOF
set terminal postscript eps enhanced color solid "Times-Roman" 16
set output 'Backbone-S2_Beta.eps'

set pm3d map  
set title 'Beta'
set palette model RGB
set xlabel 'time'
set ylabel 'pair number'

splot 'Backbone-S2_Parameters' u 1:2:4 with pm3d notitle

EOF

cat > Backbone-S2_param_Gamma.plt <<EOF
set terminal postscript eps enhanced color solid "Times-Roman" 16
set output 'Backbone-S2_Gamma.eps'

set pm3d map  
set title 'Gamma'
set palette model RGB
set xlabel 'time'
set ylabel 'pair number'

splot 'Backbone-S2_Parameters' u 1:2:5 with pm3d notitle

EOF

cat > Backbone-S2_param_Delta.plt <<EOF
set terminal postscript eps enhanced color solid "Times-Roman" 16
set output 'Backbone-S2_Delta.eps'

set pm3d map  
set title 'Delta'
set palette model RGB
set xlabel 'time'
set ylabel 'pair number'

splot 'Backbone-S2_Parameters' u 1:2:6 with pm3d notitle

EOF

cat > Backbone-S2_param_Epsil.plt <<EOF
set terminal postscript eps enhanced color solid "Times-Roman" 16
set output 'Backbone-S2_Epsil.eps'

set pm3d map  
set title 'Epsil'
set palette model RGB
